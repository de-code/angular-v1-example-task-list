    "karma-jasmine": "^0.3.8",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "phantomjs-prebuilt": "^2.1.6",
    "protractor": "^3.2.2",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
